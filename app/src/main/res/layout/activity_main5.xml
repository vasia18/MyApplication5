<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_fore"
    android:orientation="vertical"
    tools:context=".Main4Activity">

    <include layout="@layout/toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="vertical">


        <Button
            android:id="@+id/buttonFive1"
            android:layout_width="match_parent"
            android:layout_marginBottom="16dp"
            android:layout_height="48dp"
            android:background="@drawable/selector_button_fore_pressed"
            android:text="@string/button_five1" />

        <Button
            android:id="@+id/buttonFive2"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/selector_button_fore_pressed"
            android:text="@string/button_five2" />

    </LinearLayout>


</LinearLayout>